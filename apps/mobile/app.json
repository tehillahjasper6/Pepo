{
  "expo": {
    "name": "PEPO",
    "slug": "pepo-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F4B400"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.pepo.mobile",
      "infoPlist": {
        "NSCameraUsageDescription": "PEPO needs access to your camera to upload giveaway photos",
        "NSPhotoLibraryUsageDescription": "PEPO needs access to your photo library to upload giveaway photos"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F4B400"
      },
      "package": "app.pepo.mobile",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#F4B400"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}



